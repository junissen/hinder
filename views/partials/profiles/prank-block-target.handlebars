<tr>
	<td>{{pranker.name}}</td>
	<td>
		{{#if hinder_info.hinderObject.sound}}
			<button class="playAudio">Sound was sent<audio class="audioDiv" id="audioDiv" src={{hinder_info.asset}}></button>
		{{/if}}
		{{#if hinder_info.hinderObject.photo}} 
			<img class='prankPhoto' src={{hinder_info.asset}}> 
			<p>Image sent</p>
		{{/if}}
		{{#if hinder_info.hinderObject.gif}} 
			<img class='prankPhoto' src={{hinder_info.asset}}> 
			<p>Gif sent</p>
		{{/if}}
		{{#if hinder_info.hinderObject.message}}
			<p class="hinderMessage">{{hinder_info.asset}}</p> <p> Message sent </p> 
		{{/if}}
	</td>
	<td>
		<div class="thumbsUpDiv">
			<button class="thumbsUp" data-id="{{id}}" data-newthumbsup={{thumbs_up}}><i class="far fa-thumbs-up"></i>
			</button>
			{{thumbs_up}}
		</div>
		<div class="thumbsDownDiv">
			<button class="thumbsDown" uk-icon="icon: arrow-down" data-id="{{id}}" data-newthumbsdown={{thumbs_down}}><i class="far fa-thumbs-down"></i>
			</button>
			{{thumbs_down}}
		</div>
	</td>
</tr>
